{
  "name": "cimmenu",
  "version": "0.1.0",
  "main": "main.js",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://git.rwth-aachen.de/acs/core/cim/pintura.git"
  },
  "scripts": {
    "generateClassStructure": "node templates/xslt/generateClassStructure.js",
    "generateHandlebars": "node templates/xslt/generateHandlebarTemplates.js --xmlDir=../data_model",
    "createIndex": "cd templates && node createIndex.js > index.js",
    "templates": "npm run generateClassStructure && npm run generateHandlebars && npm run createIndex",
    "build": "npm run templates && rollup -c -o lib/libcimmenu.js",
  },
  "devDependencies": {
    "handlebars": "^4.0.11",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.1"
  }
}
