<ul class="floating-panel-list">
    {{#each Diagram as |value diagramId|}}
        {{#each components as |value typeName|}}
            {{#sort_each_by_pintura_label this as |value componentId|}}
                <li class="list-entry">
                    <button id="{{componentId}}-components-panel-button"
                        class="row button wide-button {{this.selected}}"
                        onclick="currentCimmenu().componentInstanceSelected(this, '{{typeName}}', '{{componentId}}');">
                            <span class="fa fa-cube">
                    </span>{{this.pintura:label.text}}</button>
                </li>
            {{/sort_each_by_pintura_label}}
        {{/each}}
    {{/each}}
</ul>
