{{#each Diagram}}
  <g id="{{@key}}" name="{{pintura:name}}">
  {{#with components}}
  {{#each this as |value typeName|}}
    <g class={{typeName}} >
    {{#each this as |value componentId|}}
      <g
        id="{{pintura:rdfId}}"
        name="{{pintura:label.text}}"
        type="{{typeName}}"
        class="image">
      {{#each pintura:points}}
        <image
          x="{{x}}"
          y="{{y}}"
          xlink:href="{{../pintura:image}}"
          id="{{componentId}}-img{{@key}}"
          height="{{imageHeight}}"
          width="{{imageWidth}}"
          transform="{{../pintura:transform}}"
          imageIndex="1"
          onmousedown="onMouseDown(evt)"
          onmouseup="onMouseUp(evt)"
          onmousemove="onMouseMove(evt)"
          onmouseover="onMouseOver(evt)"
          onmouseleave="onMouseLeave(evt)"
        />
      {{/each}}
      {{#each pintura:line}}
        <line
          x1="{{x1}}"
          y1="{{y1}}"
          x2="{{x2}}"
          y2="{{y2}}"
          id="{{componentId}}-bar{{@index}}"
          class="line"
          onmouseup="onMouseUp(evt)"
          onmouseover="onMouseOver(evt)"
          onmouseleave="onMouseLeave(evt)"
        />
      {{/each}}
      </g>
    {{/each}}
    </g>
  {{/each}}
  {{/with}}
  {{#with components}}
  {{#each this as |value typeName|}}
    {{#each this as |value componentId|}}
      {{#pintura:label}}
	<rect class="svglabel-bg" id="{{componentId}}-bg0" rx="3" ry="3" />
        <text
          x="{{x}}"
          y="{{y}}"
          class="svglabel"
          type="{{typeName}}"
          id="{{componentId}}-txt0"
          onmouseup="onMouseUp(evt)"
        >{{text}}</text>
      {{/pintura:label}}
    {{/each}}
  {{/each}}
  {{/with}}
  </g>
{{/each}}

